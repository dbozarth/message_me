<div class="center container">
<div class="row">
<div class="col-md-12">
<p id="notice"><%= notice %></p>

<h1>Sessions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= link_to 'Show', session %></td>
        <td><%= link_to 'Edit', edit_session_path(session) %></td>
        <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Session', new_session_path, class: 'btn btn-primary btn-sm' %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<%= link_to 'Home', chatrooms_path, class: 'btn btn-success btn-sm' %>
</div>
</div>
</div>